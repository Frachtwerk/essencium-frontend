{
  "name": "@frachtwerk/essencium-lib",
  "version": "9.5.0",
  "description": "This is the component library for the Essencium frontend.",
  "keywords": [
    "starter",
    "boilerplate",
    "frontend",
    "react",
    "mantine"
  ],
  "homepage": "https://github.com/Frachtwerk/essencium-frontend/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/Frachtwerk/essencium-frontend/issues"
  },
